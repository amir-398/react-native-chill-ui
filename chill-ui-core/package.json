{
  "name": "react-native-chill-ui-watch",
  "version": "1.0.0",
  "scripts": {
    "android": "expo start --android",
    "ios": "expo start --ios",
    "start": "expo start --port 8321",
    "prebuild": "expo prebuild",
    "lint": "eslint . && prettier -c .",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "ts:check": "tsc --noEmit",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "generate:icons": "node tools/svg-2-obj/index.js && prettier --write src/constants/ICONS.ts",
    "generate:flags": "node tools/flags/index.js && prettier --write src/components/inputs/phoneNumberInput/flags/index.ts",
    "test": "bunx jest",
    "test:watch": "bunx jest --watch",
    "test:coverage": "bunx jest --coverage"
  },
